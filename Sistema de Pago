class Pago(ABC):
    @abstractmethod
    def procesar_pago(self, monto):
        pass


class TarjetaCredito(Pago):
    def procesar_pago(self, monto):
        return f"Pago de ${monto} procesado con tarjeta de cr√©dito."


class PayPal(Pago):
    def procesar_pago(self, monto):
        return f"Pago de ${monto} procesado con PayPal."
